        080295E8 3401   ADD r4, #0x1
        080295EA 3864   SUB r0, #0x64
        080295EC 2864   CMP r0, #0x64
        080295EE DCFB   BGT #0x80295E8
    080295F0 F7D7 FC36   BL 0x08000E60   //Roll1RN
    080295F4 0600   LSL r0 ,r0 ,#0x18
    080295F6 2800   CMP r0, #0x0
